          "text2": "<font color=\"#414141\" face=\"Lora, serif\">WATCH VIDEO</font>",
          "buttons": "<a data-app-btn=\"true\" class=\"btn btn-info\" href=\"hva-skjer.html#extHeader13-18\" data-toolbar=\"-mbrBtnMove\">SIGN UP FOR THE CHALLENGE</a>"
